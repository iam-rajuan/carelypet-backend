{
  "info": {
    "name": "CarelyPet User Cover Upload",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "User Cover",
      "item": [
        {
          "name": "Upload User Cover",
          "request": {
            "method": "POST",
            "url": {
              "raw": "{{baseUrl}}/uploads/user/cover",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "uploads",
                "user",
                "cover"
              ]
            },
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "file",
                  "type": "file",
                  "src": ""
                }
              ]
            },
            "auth": {
              "type": "bearer",
              "bearer": [
                {
                  "key": "token",
                  "value": "{{access_token}}",
                  "type": "string"
                }
              ]
            }
          }
        }
      ]
    }
  ],
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:3000/api/v1"
    },
    {
      "key": "access_token",
      "value": ""
    }
  ]
}